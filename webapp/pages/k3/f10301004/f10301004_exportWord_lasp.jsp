		<script type="text/javascript">
			mini.parse();
			var form1 = new mini.Form("form1");
    		var aaz318;
    		var abz618;
    		//标准方法接口定义
     		function setData(data){
     			data = mini.clone(data);
				aaz318 = data.aaz318;     	
				$.ajax({
     				url:"${pageContext.request.contextPath}/k3/f10301004/queryApprovalInfo.action",
     				type:"post",
     				data: {aaz318: aaz318},
     				success: function (text){
     					var data = mini.decode(text);
     					form1.setData(data);
     				}
     			});		
     		}
     		
     		function export_word(){
     			var url = "${pageContext.request.contextPath}/k3/f10301004/exportWord_lasp.action?abz618=" + abz618 + "&sid="+Math.random() ;
				window.open(url);
     		}
     		
     		function save(){
     			var data = form1.getData();
     			form1.validate();
		        if (form1.isValid() == false) {
		            return;
		        }
		        data.aaz318=aaz318;
		        data.aaz618=abz618;        
		        mini.mask({el: document.body,cls: 'mini-mask-loading',html: '处理中...'});
				$.ajax({
					url: "${pageContext.request.contextPath}/k3/f10301004/saveAf61.action",
		            type: "post",
		            data: data,
		            success: function (text){
		            	mini.unmask(document.body);
		            	if(text.success){
		            		abz618 = text.result[0].abz618;
		            		mini.showMessageBox({
		            			title: "提示",
		            			message: "立案审批信息保存成功",
		            			buttons: ["确定"],
		            			iconCls: "mini-messagebox-info",
		            			callback: function(action){
		            				$("#saveAf61").attr("disabled","disabled");
		            				$("#export").attr("disabled",false);
		            			}
		            		});
		            	}else{
		            		mini.alert(text.msg);
		            	}
		            }
				});
     		}
     		
     		function closeWin(){
				CloseWindow();
			}
			
			function CloseWindow(action) {
         		if (window.CloseOwnerWindow){
              		return window.CloseOwnerWindow(action);
         		}else{
             		window.close();
         		}            
     		}
     		
     		//返回
     		function revert(){
     			closeWin();
     		}
		</script>
